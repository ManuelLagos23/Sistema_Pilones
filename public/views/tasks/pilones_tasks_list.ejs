        <link rel="stylesheet" href="resources/css/_buttons_home.css">
        <link rel="stylesheet" href="resources/css/_cards_home.css">
        <link rel="stylesheet" href="resources/css/_cards_pagination_home.css">







     
    </head>

    <body>
        <%- include('../partials/navbar/_navbar') %>
        <%- include('../partials/modals/_modal_turning_wetting') %>
